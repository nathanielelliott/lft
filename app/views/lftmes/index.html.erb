<h1>Listing lftmes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Request</th>
      <th>Location</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @lftmes.each do |lftme| %>
      <tr>
        <td><%= lftme.user %></td>
        <td><%= lftme.request %></td>
        <td><%= lftme.location %></td>
        <td><%= link_to 'Show', lftme %></td>
        <td><%= link_to 'Edit', edit_lftme_path(lftme) %></td>
        <td><%= link_to 'Destroy', lftme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lftme', new_lftme_path %>
